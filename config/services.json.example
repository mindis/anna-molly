{  
    "tukeys_filter": {
        "service1": {
            "q25": "service.quartil_25",
            "q75": "service.quartil_75",
            "iqr_scaling": 2,
            "metrics_regex": "system.*",
            "default": 0
        }
    },
    "SeasonalDecomposition": {
        "stl_service1": {
            "metric": "system.loadavg",
            "period_length": 6,
            "seasons": 2,
            "error_type": "norm",
            "default": 0,
            "error_history_length": 100,
            "error_handling": "tukey",
            "error_params": {
                "scaling": 2,
                "min_th_up": 0.1,
                "max_th_up": 0.9,
                "min_th_low": -0.1,
                "max_th_low": -1
            }
        }
    }
}